<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
<sheets>




	<cinemaConfig>
		<ROOT>
			<TCEforms>
			<sheetTitle>LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:cinemaConfig</sheetTitle>
			</TCEforms>

			<type>array</type>
			<el>


				<cinema>
				<TCEforms>
				<label>LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:cinemas</label>
					<config>
						<type>select</type>
						<foreign_table>tt_address</foreign_table>
						<foreign_table_where> AND tt_address.pid = ###STORAGE_PID### </foreign_table_where>
						<size>5</size>
						<minitems>1</minitems>
						<maxitems>10</maxitems>
					</config>
				</TCEforms>
				</cinema>



				<special>
				<TCEforms>
					<label>LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:onlyShowType</label>
					<config>
						<type>select</type>
						<items>
							<numIndex index="0">
								<numIndex index="0">LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:singleView</numIndex>
								<numIndex index="1">-1</numIndex>
							</numIndex>
							<numIndex index="1">
								<numIndex index="0">LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:divider</numIndex>
								<numIndex index="1">--div--</numIndex>
							</numIndex>
						</items>

						<exclusiveKeys>-1</exclusiveKeys>

						<foreign_table>tx_tmdcinema_showtype</foreign_table>
						<foreign_table_where> AND tx_tmdcinema_showtype.pid = ###STORAGE_PID### </foreign_table_where>
						<size>5</size>
						<minitems>0</minitems>
						<maxitems>10</maxitems>
					</config>
				</TCEforms>
				</special>


				<previewMin>
				<TCEforms>
					<label>LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:weeksAheadMin</label>
					<config>
						<type>input</type>
						<size>5</size>
						<eval>int</eval>
						<default>1</default>
						<range><lower>0</lower></range>
					</config>
				</TCEforms>
				</previewMin>

				<previewMax>
				<TCEforms>
				<label>LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:weeksAheadMax</label>
					<config>
						<type>input</type>
						<size>5</size>
						<eval>int</eval>
						<default>5</default>
						<range><lower>1</lower></range>
					</config>
				</TCEforms>
				</previewMax>

			</el>
		</ROOT>
	</cinemaConfig>



	<template>
		<ROOT>
			<TCEforms>
			<sheetTitle>LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:templatesLinks</sheetTitle>
			</TCEforms>

			<type>array</type>
			<el>

				<mode>
					<TCEforms>
					<label>LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:mode</label>
					<config>
						<type>select</type>
						<items>
							<numIndex index="0">
								<numIndex index="0">LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:showProgram</numIndex>
								<numIndex index="1">showProgram</numIndex>
							</numIndex>

							<numIndex index="5">
								<numIndex index="0">LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:newsFeed</numIndex>
								<numIndex index="1">RSS</numIndex>
							</numIndex>

							<numIndex index="6">
								<numIndex index="0">LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:booking</numIndex>
								<numIndex index="1">booking</numIndex>
							</numIndex>

							<numIndex index="7">
								<numIndex index="0">LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:tipafriend</numIndex>
								<numIndex index="1">tipAFriend</numIndex>
							</numIndex>

							<numIndex index="8">
								<numIndex index="0">LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:trailer</numIndex>
								<numIndex index="1">trailer</numIndex>
							</numIndex>

						</items>
					</config>
				</TCEforms>
				</mode>

					<templateFile>
						<TCEforms>
							<label>LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:templateFile</label>
							<config>
								<type>select</type>
<!--
								<items type="array">
										<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:tt_address/pi1/locallang_ff.xml:pi1_flexform.templateFile.I.0</numIndex>
										<numIndex index="1">default</numIndex>
										<numIndex index="2">EXT:tt_address/res/default.gif</numIndex>
									</numIndex>
								</items>
-->
								<itemsProcFunc>tx_tmd_cinema_addfilestosel->main</itemsProcFunc>
							</config>
						</TCEforms>
					</templateFile>


				<previewNote>
				<TCEforms>
				<label>LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:noPrg</label>
					<config>
						<type>input</type>
						<!-- default>LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:noPrgText</default geht das nicht?-->
						<default>Das Programm ab %s ist noch nicht bekannt.</default>
					</config>
				</TCEforms>
				</previewNote>



			</el>
		</ROOT>
	</template>


	<imageLinks>
		<ROOT>
			<TCEforms>
			<sheetTitle>LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:imageLinks</sheetTitle>
			</TCEforms>

			<type>array</type>
			<el>

				<pageSingelView>
				<TCEforms>
					<exclude>1</exclude>
					<label>LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:pageSingelView</label>
					<config>
						<type>group</type>
						<internal_type>db</internal_type>
						<allowed>pages</allowed>
						<size>1</size>
						<maxitems>1</maxitems>
						<minitems>0</minitems>
						<show_thumbs>0</show_thumbs>
					</config>
				</TCEforms>
				</pageSingelView>

				<pageProgram>
				<TCEforms>
					<exclude>1</exclude>
					<label>LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:pageProgramView</label>
					<config>
						<type>group</type>
						<internal_type>db</internal_type>
						<allowed>pages</allowed>
						<size>1</size>
						<maxitems>1</maxitems>
						<minitems>0</minitems>
						<show_thumbs>0</show_thumbs>
					</config>
				</TCEforms>
				</pageProgram>

				<pagePreview>
				<TCEforms>
					<exclude>1</exclude>
					<label>LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:pagePreview</label>
					<config>
						<type>group</type>
						<internal_type>db</internal_type>
						<allowed>pages</allowed>
						<size>1</size>
						<maxitems>1</maxitems>
						<minitems>0</minitems>
						<show_thumbs>0</show_thumbs>
					</config>
				</TCEforms>
				</pagePreview>

				<pageTipAFriend>
				<TCEforms>
					<exclude>1</exclude>
					<label>LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:pageTipAFriend</label>
					<config>
						<type>group</type>
						<internal_type>db</internal_type>
						<allowed>pages</allowed>
						<size>1</size>
						<maxitems>1</maxitems>
						<minitems>0</minitems>
						<show_thumbs>0</show_thumbs>
					</config>
				</TCEforms>
				</pageTipAFriend>

				<pageBooking>
				<TCEforms>
					<exclude>1</exclude>
					<label>LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:pageBooking</label>
					<config>
						<type>group</type>
						<internal_type>db</internal_type>
						<allowed>pages</allowed>
						<size>1</size>
						<maxitems>1</maxitems>
						<minitems>0</minitems>
						<show_thumbs>0</show_thumbs>
					</config>
				</TCEforms>
				</pageBooking>

				<width>
				<TCEforms>
				<label>LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:width</label>
					<config>
						<type>input</type>
						<size>10</size>
						<default></default>
					</config>
				</TCEforms>
				</width>

				<height>
				<TCEforms>
				<label>LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:height</label>
					<config>
						<type>input</type>
						<size>10</size>
						<default></default>
					</config>
				</TCEforms>
				</height>

				<clickEnlarge>
				<TCEforms>
					<label>LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:clickEnlarge</label>
					<config>
						<type>check</type>
					</config>
				</TCEforms>
				</clickEnlarge>

				<disallowBooking>
				<TCEforms>
				<label>LLL:EXT:tmd_cinema/pi1/locallang_ff.xml:noBooking</label>
					<config>
						<type>check</type>
					</config>
				</TCEforms>
				</disallowBooking>


			</el>
		</ROOT>
	</imageLinks>




</sheets>
</T3DataStructure>